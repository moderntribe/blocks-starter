{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"slug": "dark",
	"title": "Dark",
	"blockTypes": [ "core/group" ],
	"styles": {
		"color": {
			"background": "var(--wp--preset--color--base-black)",
			"text": "var(--wp--preset--color--base-white)"
		},
		"blocks": {
			"core/button": {
				"variations": {
					"outlined": {
						"border": {
							"color": "var(--wp--preset--color--base-white)"
						},
						"color": {
							"text": "var(--wp--preset--color--base-white)"
						},
						"css": ":hover{background-color: var(--wp--preset--color--base-white); color: var(--wp--preset--color--base-black)} :focus{background-color: var(--wp--preset--color--base-white); color: var(--wp--preset--color--base-black)}"
					},
					"ghost": {
						"color": {
							"text": "var(--wp--preset--color--base-white)"
						},
						"css": ":hover{color: var(--color-neutral-20)} :focus{color: var(--color-neutral-20)}"
					}
				}
			}
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--base-white)",
					"text": "var(--wp--preset--color--base-black)"
				},
				":hover": {
					"color": {
						"background": "var(--color-neutral-20)",
						"text": "var(--wp--preset--color--base-black)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--color-neutral-20)",
						"text": "var(--wp--preset--color--base-black)"
					}
				}
			}
		}
	}
}
