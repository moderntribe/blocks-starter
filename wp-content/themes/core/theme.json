				},
				{
					"color": "#84bd00",
					"name": "Success",
					"slug": "success"
				}
			]
		},
		"custom": {
			"fontWeight": {
				"regular": 400,
				"bold": 700
			},
			"spacing": {
				"grid-gutter": "clamp(1.5rem, 1.2rem + 1.5vw, 3rem)",
				"grid-margin": "clamp(1rem, 0rem + 5vw, 6rem)"
			}
		},
		"layout": {
			"contentSize": "640px",
			"wideSize": "calc(100% - var(--wp--custom--spacing--grid-margin) * 2)"
		},
		"useRootPaddingAwareAlignments": true,
		"spacing": {
			"margin": true,
			"padding": true,
			"spacingSizes": [
				{
					"size": "clamp(0.25rem, 0.2rem + 0.25vw, 0.5rem)",
					"slug": "10",
					"name": "XX Small"
				},
				{
					"size": "clamp(0.5rem, 0.4rem + 0.5vw, 1rem)",
					"slug": "20",
					"name": "X Small"
				},
				{
					"size": "clamp(1rem, 0.8rem + 1vw, 2rem)",
					"slug": "30",
					"name": "Small"
				},
				{
					"size": "clamp(1.5rem, 1.2rem + 1.5vw, 3rem)",
					"slug": "40",
					"name": "Medium"
				},
				{
					"size": "clamp(2rem, 1.6rem + 2vw, 4rem)",
					"slug": "50",
					"name": "Large"
				},
				{
					"size": "clamp(3rem, 2.4rem + 3vw, 6rem)",
					"slug": "60",
					"name": "X Large"
				},
				{
					"size": "clamp(3.75rem, 3rem + 3.75vw, 7.5rem)",
					"slug": "70",
					"name": "XX Large"
				}
			]
		},
		"typography": {
			"customFontSize": false,
			"dropCap": false,
			"fluid": false,
			"fontStyle": false,
			"fontWeight": false,
			"letterSpacing": false,
			"lineHeight": false,
			"textDecoration": false,
			"fontFamilies": [
				{
					"fontFamily": "\"Roboto\", sans-serif",
					"name": "Roboto",
					"slug": "roboto",
					"fontFace": [
						{
							"fontFamily": "Roboto",
							"fontDisplay": "swap",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/Roboto-Regular.woff2",
								"file:./assets/fonts/Roboto-Regular.woff"
							]
						},
						{
							"fontFamily": "Roboto",
							"fontDisplay": "swap",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/Roboto-Italic.woff2",
								"file:./assets/fonts/Roboto-Italic.woff"
							]
						},
						{
							"fontFamily": "Roboto",
							"fontDisplay": "swap",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/Roboto-Bold.woff2",
								"file:./assets/fonts/Roboto-Bold.woff"
							]
						},
						{
							"fontFamily": "Roboto",
							"fontDisplay": "swap",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/Roboto-BoldItalic.woff2",
								"file:./assets/fonts/Roboto-BoldItalic.woff"
							]
						}
					]
				}
			],
			"fontSizes": [
				{
					"slug": "90",
					"size": "clamp(3rem, 2.4rem + 3vw, 6rem)",
					"name": "Font Size 90"
				},
				{
					"slug": "80",
					"size": "clamp(2.5rem, 2.05rem + 2.25vw, 4.75rem)",
					"name": "Font Size 80"
				},
				{
					"slug": "70",
					"size": "clamp(2rem, 1.6rem + 2vw, 4rem)",
					"name": "Font Size 70"
				},
				{
					"slug": "60",
					"size": "clamp(1.5rem, 1.2rem + 1.5vw, 3rem)",
					"name": "Font Size 60"
				},
				{
					"slug": "50",
					"size": "clamp(1.375rem, 1.15rem + 1.125vw, 2.5rem)",
					"name": "Font Size 50"
				},
				{
					"slug": "40",
					"size": "clamp(1.25rem, 1.1rem + 0.75vw, 2rem)",
					"name": "Font Size 40"
				},
				{
					"slug": "30",
					"size": "clamp(1.125rem, 1.05rem + 0.375vw, 1.5rem)",
					"name": "Font Size 30"
				},
				{
					"slug": "20",
					"size": "clamp(1rem, 0.95rem + 0.25vw, 1.25rem)",
					"name": "Font Size 20"
				},
				{
					"slug": "10",
					"size": "clamp(0.875rem, 0.85rem + 0.125vw, 1rem)",
					"name": "Font Size 10"
				}
			]
		},
		"shadow": {
			"presets": [
				{
					"name": "Default",
					"slug": "default",
					"shadow": "0px 0px 1px rgba(48, 49, 51, 0.05), 0px 8px 16px rgba(48, 49, 51, 0.1)"
				},
				{
					"name": "Hover",
					"slug": "hover",
					"shadow": "0px 0px 1px rgba(48, 49, 51, 0.05), 0px 8px 50px rgba(48, 49, 51, 0.1)"
				}
			]
		},
		"blocks": {
			"core/button": {
				"border": {
					"color": false,
					"radius": false,
					"style": false,
					"width": false
				},
				"typography": {
					"fontFamilies": [],
					"fontSizes": [],
					"textTransform": false
				}
			},
			"core/group": {
				"color": {
					"background": true
				}
			},
			"core/heading": {
				"typography": {
					"fontFamilies": [],
					"fontSizes": []
				}
			},
			"core/list": {
				"typography": {
					"fontFamilies": [],
					"fontSizes": []
				}
			},
			"core/list-item": {
				"typography": {
					"fontFamilies": [],
					"fontSizes": []
				}
			},
			"core/paragraph": {
