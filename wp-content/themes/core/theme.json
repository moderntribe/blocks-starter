	"styles": {
		"color": {
			"text": "var(--wp--preset--color--base-black)",
			"background": "var(--wp--preset--color--base-white)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--roboto)",
			"fontSize": "var(--wp--preset--font-size--20)",
			"fontStyle": "normal",
			"fontWeight": "var(--wp--custom--font-weight--regular)",
			"lineHeight": "1.5"
		},
		"blocks": {
			"core/paragraph": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--20)",
					"fontWeight": "var(--wp--custom--font-weight--regular)",
					"lineHeight": "1.5"
				}
			},
			"core/separator": {
				"border": {
					"bottom": {
						"width": "1px"
					}
				},
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--40)",
						"bottom": "var(--wp--preset--spacing--40)"
					}
				}
			},
			"core/list": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--20)",
					"fontWeight": "var(--wp--custom--font-weight--regular)",
					"lineHeight": "1.5"
				}
			},
			"core/pullquote": {
				"border": {
					"top": {
						"width": "3px",
						"style": "solid",
						"color": "currentcolor"
					},
					"bottom": {
						"width": "3px",
						"style": "solid",
						"color": "currentcolor"
					}
				},
				"spacing": {
					"margin": {
						"top": "2rem",
						"bottom": "2rem"
					},
					"padding": {
						"top": "var(--wp--preset--spacing--40)",
						"bottom": "var(--wp--preset--spacing--40)"
					}
				},
				"elements": {
					"cite": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--20)",
							"fontWeight": "var(--wp--custom--font-weight--regular)",
							"lineHeight": "1.5",
							"textTransform": "none",
							"fontStyle": "normal"
						}
					}
				}
			},
			"core/quote": {
				"spacing": {
					"margin": {
						"top": "2rem",
						"bottom": "2rem"
					}
				},
				"elements": {
					"cite": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--20)",
							"fontWeight": "var(--wp--custom--font-weight--regular)",
							"lineHeight": "1.5",
							"textTransform": "none",
							"fontStyle": "normal"
						}
					}
				}
			},
			"core/table": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--20)",
					"lineHeight": "1.5"
				},
				"elements": {
					"caption": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--10)",
							"fontWeight": "var(--wp--custom--font-weight--regular)",
							"lineHeight": "1.65"
						}
					}
				}
			},
			"core/query-pagination": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--20)",
					"fontWeight": "var(--wp--custom--font-weight--regular)",
					"lineHeight": "1.5"
				}
			}
		},
		"elements": {
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto)",
					"fontWeight": "var(--wp--custom--font-weight--bold)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--80)",
					"lineHeight": "1.1"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--70)",
					"lineHeight": "1.12"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--60)",
					"lineHeight": "1.15"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--50)",
					"lineHeight": "1.2"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--40)",
					"lineHeight": "1.25"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--30)",
					"lineHeight": "1.4"
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--neutral-70)"
				},
				"spacing": {
					"margin": {
						"top": "1rem"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--10)",
					"fontWeight": "var(--wp--custom--font-weight--regular)",
					"lineHeight": "1.65"
				}
			},
			"cite": {
				"spacing": {
					"margin": {
						"top": "1rem"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--20)",
					"fontWeight": "var(--wp--custom--font-weight--regular)",
					"lineHeight": "1.5"
				}
			},
			"button": {
				"border": {
					"radius": "100px"
				},
				"color": {
					"background": "var(--themed-button-background-color)",
					"text": "var(--themed-button-text-color)"
				},
				"spacing": {
					"padding": {
						"top": "0.7813rem",
						"right": "1.5625rem",
						"bottom": "0.7813rem",
						"left": "1.5625rem"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--20)",
					"fontWeight": "var(--wp--custom--font-weight--bold)",
					"lineHeight": "1.3"
				},
				":hover": {
					"color": {
						"background": "var(--themed-button-hover-background-color)",
						"text": "var(--themed-button-hover-text-color)"
					}
				},
				":focus": {
					"color": {
						"background": "var(--themed-button-hover-background-color)",
						"text": "var(--themed-button-hover-text-color)"
					}
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--custom--spacing--grid-gutter)",
			"padding": {
				"top": "0em",
				"right": "var(--wp--custom--spacing--grid-margin)",
				"bottom": "0em",
				"left": "var(--wp--custom--spacing--grid-margin)"
			}
		}
	}
}
